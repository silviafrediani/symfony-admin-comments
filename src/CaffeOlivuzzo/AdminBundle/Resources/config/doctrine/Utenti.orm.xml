<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="CaffeOlivuzzo\AdminBundle\Entity\Utenti" table="utenti">
    <id name="username" type="string" column="username" length="45">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="password" type="string" column="password" length="200" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="denominazione" type="string" column="denominazione" length="80" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="ultimoAccesso" type="datetime" column="ultimo_accesso" nullable="true"/>
  </entity>
</doctrine-mapping>
